{
	"compilerOptions": {
		"allowJs": true,
		"target": "es5",
		"moduleResolution": "node",
		"noLib": false,
		"lib": ["ESNext"],
		"sourceMap": true,
		// "module": "commonjs",
		"module": "ESNext",
		"strict": true,
		"resolveJsonModule": true,
		"declaration": true,
		"noImplicitOverride": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true
	}
}
